{
  "dashboard": {
    "title": "E-Commerce Cache Performance Dashboard",
    "tags": ["cache", "performance", "ecommerce"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Cache Hit Ratio (%)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(cache_hit_total[5m]) / (rate(cache_hit_total[5m]) + rate(cache_miss_total[5m])) * 100",
            "legendFormat": "Cache Hit Ratio"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100}
        ]
      },
      {
        "id": 2,
        "title": "Response Time (P95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_server_requests_seconds_bucket[5m]))",
            "legendFormat": "P50 Latency"
          }
        ],
        "yaxes": [
          {"format": "s", "min": 0}
        ]
      },
      {
        "id": 3,
        "title": "Database Queries Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(database_query_total[5m]) * 60",
            "legendFormat": "DB Queries/min"
          }
        ],
        "yaxes": [
          {"format": "short", "min": 0}
        ]
      },
      {
        "id": 4,
        "title": "Requests per Second",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "yaxes": [
          {"format": "short", "min": 0}
        ]
      },
      {
        "id": 5,
        "title": "Cache Hit vs Miss",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(cache_hit_total[5m]) * 60",
            "legendFormat": "Cache Hits/min"
          },
          {
            "expr": "rate(cache_miss_total[5m]) * 60",
            "legendFormat": "Cache Misses/min"
          }
        ],
        "yaxes": [
          {"format": "short", "min": 0}
        ]
      }
    ],
    "refresh": "5s",
    "time": {
      "from": "now-30m",
      "to": "now"
    }
  }
}